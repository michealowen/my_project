<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hibernate.jpa.test.pack.war">

    <class name="entity.record" table="B_LEASEINFOHIS_SUM">
        <id name="leasestation" type="java.lang.Integer"  column="LEASESTATION" length="4">
            <generator class="assigned"/>
        </id>
        <property name="returnstation" type="java.lang.Integer" column="RETURNSTATION"/>
        <property name="time" type="java.lang.Integer" column="LEASETIME"/>
        <property name="leasedate" type="java.lang.Date" column="LEASEDATE"/>
        <property name="num" type="java.lang.Integer" column="BIKENUM"/>
    </class>

</hibernate-mapping>

