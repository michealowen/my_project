<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
     <class name="entity.Train" table="train_copy_copy">
     <id name="id" column="id"> 
       <generator class="assigned"/>
     </id>
     <property name="Train_number" column="Train_number"/>
     <property name="Train_type"   column="Train_type"/>
     <property name="Start_station"  column="Start_station"/>
     <property name="Term_station"  column="Term_station"/>
     <property name="Pass_station"  column="Pass_station"/>
     <property name="Tickets"  column="Tickets"/>
     <property name="Waittime"  column="Waittime"/>
     <property name="Start_time" column="Start_time"></property>
     </class>
</hibernate-mapping>



